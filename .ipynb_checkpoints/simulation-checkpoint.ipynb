    "from vpython import *\n",
    "\n",
    "scene = canvas()\n",
    "scene.title = \"Predator Prey Simulation\"\n",
    "scene.width = 800\n",
    "scene.height = 600\n",
    "\n",
    "prey = sphere(pos = vector(0,0,0), radius = 1, color = color.green)\n",
    "\n",
    "for i in range(300):\n",
    "   rate(30)\n",
    "   prey.pos.x += 0.1\n",
    "   if prey.pos.x > 10: prey.pos.x = -10"
